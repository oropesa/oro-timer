{
  "name": "oro-timer",
  "version": "2.2.2",
  "description": "OroTimer is a class designed for monitoring code performance.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "coverage:open": "node coverage.open.js",
